            p(style="display: inline;") #{object.time}  
          else
            p(style="display: inline;") &nbsp&nbsp&nbsp&nbsp&nbsp&nbsp       
          if(object.leftDoubleBarLine)
            p(style="display: inline;")   ||
          else
            p(style="display: inline;")   &nbsp&nbsp
          p(style="display: inline;") #{object.chords} |  
          if(object.rightDoubleBarLine)
