extends tablelayout

block content
  h1 #{title}
  - if(error)
    div.alert-error #{error}
  p Style: #{style}
  p Composer: #{composer}
  p Key: #{key}

  table
    tr
      each object in line1
        -if(object.rehearsalLetter)
          p &nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp [ #{object.rehearsalLetter} ]
        -if(object.time)
          td #{object.time} &nbsp
        -if(object.leftDoubleBarLine) 
          td || #{object.chords} &nbsp ||
        -else
          td #{object.chords} &nbsp | &nbsp
        -if(object.rightDoubleBarLine)
          p(style="display: inline;") |
          


  ul(style="list-style-type: none;")
    li
      - if (line1)
        each object in line1
          if(object.rehearsalLetter)
            p &nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp [ #{object.rehearsalLetter} ]
          if(object.time)
            p(style="display: inline;") #{object.time}  
          else
            p(style="display: inline;") &nbsp&nbsp&nbsp&nbsp&nbsp&nbsp  
          if(object.leftDoubleBarLine)
            p(style="display: inline;")   || 
          else
            p(style="display: inline;")   &nbsp&nbsp
          p(style="display: inline;") #{object.chords} |      
    br
    br
    li
      - if (line2)
        each object in line2
          if(object.rehearsalLetter)
            p &nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp [ #{object.rehearsalLetter} ]
